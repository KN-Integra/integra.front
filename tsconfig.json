{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "allowJs": false,
    "sourceMap": true,
    "strict": true,
    "experimentalDecorators": true,
    "noImplicitThis": true,
    "types": [
      "@nuxt/types",
      "@nuxt/content",
    ]
  }
}
